{
  "appId": "com.aparajita.capacitor.splashscreendemo",
  "appName": "Splash",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "dist",
  "hideLogs": false,
  "plugins": {
    "Keyboard": {
      "resize": "body"
    },
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "WSSplashScreen": {
      "animated": true,
      "source": "*",
      "backgroundColor": "systemBackground",
      "android": {
        "fullscreen": false
      }
    }
  },
  "cordova": {}
}
